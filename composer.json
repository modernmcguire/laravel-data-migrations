{
    "name": "modernmcguire/laravel-data-migrations",
    "description": "An extension to allow you to separate data migrations from structure migrations",
    "license": "MIT",
    "authors": [{
        "name": "Jose Lorente",
        "email": "jose.lorente.martin@gmail.com"
    }, {
        "name": "Eran Machiels",
        "email": "dev@eranmachiels.nl"
    }],
    "homepage": "https://github.com/modernmcguire/laravel-data-migrations",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "ModernMcguire\\DataMigrations\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ModernMcguire\\DataMigrations\\DataMigrationsServiceProvider"
            ]
        }
    }
}
